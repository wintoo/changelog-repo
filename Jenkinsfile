node {
    
    stage('checkout branch '+ "${BRANCH_NAME}" ){
        println ("Branch Name =" + "${BRANCH_NAME}")
    }
    
    stage('First stage'){
        echo 'first stage'
    }
    
    stage('second stage') {
        echo 'second stage'
    }
    
    stage('third stage') {
        echo 'third stage'
    }
}
